{{>layout/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
        <th scope="col">created_at</th>
        <th scope="col">updated_at</th>
        <th scope="col">category</th>

    </tr>
    </thead>
    <tbody>

    {{#feeds}}
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>

            <td>{{createdAt}}</td>
            <td>{{updatedAt}}</td>
            <td>{{category}}</td>



        </tr>
    {{/feeds}}
    </tbody>
</table>


{{>layout/footer}}

